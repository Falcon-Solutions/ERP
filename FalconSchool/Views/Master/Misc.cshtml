        <div class="col-5">
            <div class="form-group ">
                <div class="card">
                    <h5 class="card-header hidden"></h5>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered first">
                                <thead>
                                    <tr>
                                        <th class="hidden">Id</th>
                                        <th>RelationShip Name</th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.relationshipList)
                                    {
                                        <tr>
                                            <td class="hidden">@item.Id</td>
                                            <td class="">@item.Name</td>
                                            <td class="wd-action-btn-1">

                                                @Html.ActionLink("Edit", "Edit", "Relationship", new { id = item.Id }, new { @class = "btn btn-primary btn-sm" })
                                                @Html.ActionLink("Delete", "Delete", "Relationship", new { id = item.Id }, new { @class = "btn btn-primary btn-sm" })

                                            </td>
                                        </tr>
                                    }

                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>

            </div>
            <p>
                @Html.ActionLink("Create New", "Create", "RelationShip", null, new { @class = "btn btn-primary" })
                @Html.ActionLink("Back", "Home", "Dashboard", null, new { @class = "btn btn-primary" })
            </p>
